        "editor.invisible": null,
        "editor.wrap_guide": "#acb0be",
        "editor.active_wrap_guide": "#acb0be",
        "editor.document_highlight.write_background": "#ffdd801F",
        "editor.document_highlight.read_background": "#719aad3F",

        "terminal.background": "#000000",
        "terminal.foreground": "#ffffffDF",
        "terminal.ansi.bright_black": "#ffffff4f",

        "conflict": "#ffb86c",
        "conflict.background": "#3a3f41",
        "conflict.border": "#ffb86c",
        "created": "#50fa7b",
