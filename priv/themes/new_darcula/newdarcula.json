{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "New Darcula",
  "author": "Evan Simpson <evan@evan.news>",
  "themes": [
    {
      "name": "New Darcula",
      "appearance": "dark",
      "style": {
        "border": "#f9f9f928",
        "border.variant": "#f9f9f928",
        "border.focused": "#f9f9f928",
        "border.selected": "#f9f9f928",
        "border.transparent": "#f9f9f928",
        "border.disabled": "#acb0be",

        "elevated_surface.background": "#37393b",
        "drop_target.background": "#c99e475F",

        "surface.background": "#43474d",
        "background": "#f9f9f928",

        "element.background": "#f9f9f908",
        "element.hover": "#FFFFFF1B",
        "element.active": "#f9f9f93F",
        "element.selected": "#85a7ff0F",
        "element.disabled": "#000000",

        "ghost_element.background": null,
        "ghost_element.hover": "#FFFFFF0F",
        "ghost_element.active": "#FFFFFF1F",
        "ghost_element.selected": "#85a7ff0F",
        "ghost_element.disabled": null,

        "text": "#FFFFFF",
        "text.muted": "#FFFFFF9F",
        "text.placeholder": "#ACB0BE",
        "text.disabled": "#FFFFFF4F",
        "text.accent": "#3694ff",

        "icon": "#4c4f69",
        "icon.muted": null,
        "icon.disabled": null,
        "icon.placeholder": null,
        "icon.accent": "#b78dc2",

        "status_bar.background": "#37393b",
        "title_bar.background": "#37393b",
        "tab_bar.background": "#2b2b2d",
        "tab.inactive_background": "#3b3f4100",
        "tab.active_background": "#37393b",
        "search.match_background": "#2196204F",
        "toolbar.background": "#37393b",
        "panel.background": "#37393b",
        "panel.focused_border": "#f9f9f928",
        "pane.focused_border": "#f9f9f928",

        "scrollbar.thumb.background": "#acb0be80",
