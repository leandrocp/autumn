          {
            "cursor": "#b9dded",
            "selection": "#91d1ed1F"
          }
        ],

        "syntax": {
          "attribute": {
            "color": "#e1afe3",
            "font_style": null,
            "font_weight": null
          },
          "comment": {
            "color": "#787c80",
            "font_style": null,
            "font_weight": null
