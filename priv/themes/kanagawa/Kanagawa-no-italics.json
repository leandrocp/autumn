{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Kanagawa - no italics",
  "author": "Ethan Gilmore <ethanmckgilmore@gmail.com>",
  "themes": [
    {
      "name": "Kanagawa Wave - No Italics",
      "appearance": "dark",
      "style": {
        // Title and status bars
        "title_bar.background": "#16161D",
        "status_bar.background": "#16161D",

        // Tab bar
        "tab_bar.background": "#16161D",
        "tab.inactive_background": "#1F1F28",
        "tab.active_background": "#2A2A37",
        "toolbar.background": "#2A2A37",

        // Editor
        "editor.background": "#1F1F28",
        "editor.gutter.background": "#2A2A37",
        "editor.active_line.background": "#363646",
        "editor.foreground": "#DCD7BA",
        "editor.line_number": "#54546D",
        "editor.active_line_number": "#DCD7BA",
        "predictive": "#727169",

        // Other panels
        "panel.background": "#1F1F28",
        "elevated_surface.background": "#2A2A37",
        "search.match_background": "#363646",
        "element.background": "#1F1F28",
        "element.hover": "#363646",

        // Borders
        "border": "#363646",
        "border.variant": "#363646",

        // UI Text
        "text": "#DCD7BA",
        "text.muted": "#727169",
        "text.placeholder": "#727169",
        "text.disabled": "#54546D",
        "text.accent": "#FFA066",
        "icon": "#DCD7BA",
        "icon.muted": "#727169",
        "icon.placeholder": "#727169",
        "icon.disabled": "#54546D",
        "icon.accent": "#FFA066",

        // Diagnostics
        "error": "#E82424",
        "error.border": "#E82424",
        "error.background": "#E82424",
        "warning": "#FF9E3B",
        "warning.border": "#FF9E3B",
        "warning.background": "#FF9E3B",
        "info": "#658594",
        "info.border": "#658594",
        "info.background": "#658594",
        "hint": "#6A9589",
        "hint.border": "#6A9589",
        "hint.background": "#6A9589",

        // Git
        "created": "#76946A",
        "created.border": "#76946A",
        "created.background": "#76946A",
        "deleted": "#C34043",
        "deleted.border": "#C34043",
        "deleted.background": "#C34043",
        "modified": "#DCA561",
        "modified.border": "#DCA561",
        "modified.background": "#DCA561",
        "ignored": "#658594",
        "ignored.border": "#658594",
        "ignored.background": "#658594",

        // Terminal
        "terminal.background": "#1F1F28",
        "terminal.ansi.black": "16161D",
        "terminal.ansi.red": "#C34043",
        "terminal.ansi.green": "#76946A",
        "terminal.ansi.yellow": "#C0A36E",
        "terminal.ansi.blue": "#7E9CD8",
        "terminal.ansi.magenta": "#957FB8",
        "terminal.ansi.cyan": "#6A9589",
        "terminal.ansi.white": "#C8C093",
        "terminal.ansi.bright_black": "#727169",
        "terminal.ansi.bright_red": "#E82424",
        "terminal.ansi.bright_green": "#98BB6C",
        "terminal.ansi.bright_yellow": "#E6C384",
        "terminal.ansi.bright_blue": "#7FB4CA",
        "terminal.ansi.bright_magenta": "#938AA9",
        "terminal.ansi.bright_cyan": "#7AA89F",
        "terminal.ansi.bright_white": "#DCD7BA",

        // Cursors
        "players": [
          {
            "cursor": "#DCD7BA",
            "selection": "#363646"
          },
          {
            "cursor": "#DCA561",
            "selection": "#49443C"
          },
          {
            "cursor": "#76946A",
            "selection": "#2B3328"
          },
          {
            "cursor": "#C34043",
            "selection": "#43242B"
          }
        ],

        // Syntax highlighting
        "syntax": {
          "attribute": {
            "color": "#FFA066",
            "font_style": null,
            "font_weight": null
          },
          "boolean": {
            "color": "#FFA066",
            "font_style": null,
            "font_weight": null
          },
          "comment": {
            "color": "#727169",
            "font_style": "italic",
            "font_weight": null
          },
          // "comment.doc": {
          //   "color": "#FF0000",
